.cate-section {
  width: 712rpx;
  position: relative;
  z-index: 5;
  border-radius: 16rpx 16rpx 0 0;
  margin-top: -20rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-wrap: wrap;
  padding: 40rpx 22rpx;
  background: #f7fff0;
  font-size: 26rpx;
  border-top: 2rpx solid #89e3687D;
  border-bottom: 2rpx solid #8aaa57;
}
